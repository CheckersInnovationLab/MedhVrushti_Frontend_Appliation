    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
       android:gravity="center"
         >

        <com.airbnb.lottie.LottieAnimationView
            android:id="@+id/subscription_notSubscribe_lottie"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_300sdp"
            app:lottie_autoPlay="true"
            app:lottie_loop="true"
            android:layout_marginTop="-30dp"
            app:lottie_rawRes="@raw/empty_lottie" />


        <TextView
            android:id="@+id/Subscription_not_found_header_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/subscription_notSubscribe_lottie"
            android:text="No Active Subscriptions Found"
            android:textSize="@dimen/_20ssp"
            android:textColor="@color/themeColor"
            android:textStyle="bold"
            android:layout_marginLeft="@dimen/_5sdp"
            android:layout_marginRight="@dimen/_5sdp"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="-10dp"/>


        <TextView
            android:id="@+id/Subscription_not_found_message_id"
            android:layout_below="@+id/Subscription_not_found_header_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Upgrade Your Learning Journey â€“ Enroll in Our Premium Courses Today"
            android:textSize="@dimen/_15ssp"
            android:textStyle="italic"
            android:layout_marginLeft="@dimen/_5sdp"
            android:layout_marginRight="@dimen/_5sdp"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_centerHorizontal="true"
            android:gravity="center"/>

        <Button
            android:id="@+id/Subscription_unlock_button_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/Subscription_not_found_message_id"
            android:text="Unlock Subscription"
            android:layout_marginTop="@dimen/_25sdp"
            android:textSize="@dimen/_12ssp"
            android:textStyle="italic"
            android:layout_centerHorizontal="true"
            android:background="@drawable/button_back_view"
            android:backgroundTint="#028009"
            app:backgroundTint="#028009"/>
