<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F2F6FA"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    tools:context=".CompetitivePkg.Competitive_Ass_Result_Screen">


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">


            <androidx.appcompat.widget.Toolbar
                android:id="@+id/AssessmentTopic_toolbar_id"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_40sdp"
                android:background="#344AC1"
                app.background="#344AC1"
                android:minHeight="?attr/actionBarSize"
                android:theme="?attr/actionBarTheme" >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"

                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/AssessmentTopicToolbar_back_button"
                        android:layout_width="@dimen/_25sdp"
                        android:layout_height="@dimen/_20sdp"
                        android:layout_centerVertical="true"
                        android:layout_gravity="center_vertical"
                        android:src="@drawable/back_icon_white"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Test Result"
                        android:layout_toRightOf="@id/edit_personal_detailsToolbar_back_button"
                        android:id="@+id/edit_personal_detailsToolbar_title"
                        android:layout_marginLeft="@dimen/_10sdp"
                        android:textSize="@dimen/_18sdp"
                        android:layout_centerVertical="true"
                        android:textColor="#D2F2F6"
                        android:layout_gravity="center_vertical"
                        android:textStyle="bold"
                        />


                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>


            <androidx.cardview.widget.CardView
                android:id="@+id/card2001"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginLeft="@dimen/_5sdp"
                android:layout_marginRight="@dimen/_5sdp"
                app:cardCornerRadius="@dimen/_20sdp">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/profile_top_layout_id"
                    android:background="#344AC1">



                    <de.hdodenhof.circleimageview.CircleImageView
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:id="@+id/profile_image_id2001"
                        android:layout_width="@dimen/_80sdp"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:layout_height="@dimen/_80sdp"
