                        app:civ_border_width="2dp"
                        app:civ_border_color="#FF000000"/>

                    <TextView

                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Shubham hadawale"
                        android:textColor="#CEF5FA"
                        android:textStyle="bold"
                        android:layout_below="@+id/profile_image_id2001"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:textSize="@dimen/_15ssp"
                        android:id="@+id/Profile_Student_Name_id2001"/>
                    <RelativeLayout
                        android:id="@+id/horizontal_2001"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_1sdp"
                        android:background="#C1EFF4"
                        android:layout_below="@+id/Profile_Student_Name_id2001"
                        android:layout_marginTop="@dimen/_5sdp"/>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_80sdp"
                        android:orientation="horizontal"
                        android:layout_below="@+id/horizontal_2001"
                        android:weightSum="3">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/Current_score_id1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="20"
                                android:textColor="#CEF5FA"
                                android:textSize="@dimen/_17ssp"
                                android:textStyle="bold"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Current Score"
                                android:layout_marginTop="@dimen/_5sdp"
                                android:textColor="#CEF5FA"
                                android:textStyle="bold|italic"
                                android:textSize="@dimen/_11ssp"/>

                        </LinearLayout>
                        <RelativeLayout
                            android:layout_width="@dimen/_1sdp"
                            android:layout_height="match_parent"
                            android:background="#C1EFF4"
                            android:layout_marginTop="@dimen/_10sdp"
                            android:layout_marginBottom="@dimen/_10sdp"/>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/AssessmentResult_average_score_id"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="19"
                                android:textColor="#CEF5FA"
                                android:textSize="@dimen/_17ssp"
                                android:textStyle="bold"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Average Score"
                                android:layout_marginTop="@dimen/_5sdp"
                                android:textColor="#CEF5FA"
                                android:textStyle="bold|italic"
                                android:textSize="@dimen/_11ssp"/>

                        </LinearLayout>
                        <RelativeLayout
                            android:layout_width="@dimen/_1sdp"
                            android:layout_height="match_parent"
                            android:background="#C1EFF4"
                            android:layout_marginTop="@dimen/_10sdp"
                            android:layout_marginBottom="@dimen/_10sdp"/>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/Competitive_assessment_ranking_text_id"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="2356"
                                android:textColor="#CEF5FA"
                                android:textSize="@dimen/_17ssp"
                                android:textStyle="bold"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Ranking"
                                android:layout_marginTop="@dimen/_5sdp"
                                android:textStyle="bold|italic"
                                android:textColor="#CEF5FA"
                                android:textSize="@dimen/_11ssp"/>

                        </LinearLayout>

                    </LinearLayout>

                </RelativeLayout>
            </androidx.cardview.widget.CardView>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Performance: Learn!"
                    android:layout_marginLeft="@dimen/_10sdp"
                    android:textSize="@dimen/_20ssp"
                    android:textColor="#454444"
                    android:textStyle="bold"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="learn th topic in key focus area and try again.we know you do better"
                    android:layout_marginLeft="@dimen/_10sdp"
                    android:textSize="@dimen/_13sdp"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:layout_marginRight="@dimen/_10sdp"/>


            </LinearLayout>

    <androidx.cardview.widget.CardView
