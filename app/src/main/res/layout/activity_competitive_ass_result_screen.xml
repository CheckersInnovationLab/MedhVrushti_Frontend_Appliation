                            android:text="30"
                            android:textSize="@dimen/_15ssp"
                            android:textStyle="bold" />

                    </LinearLayout>

                </RelativeLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="@dimen/_10sdp"
                    android:layout_weight="1">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="@dimen/_25sdp"
                            android:layout_height="@dimen/_25sdp"
                            android:src="@drawable/correct_icon"/>

                        <TextView
                            android:id="@+id/total_correct_question_id"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="0"
                            android:layout_marginLeft="@dimen/_2sdp"
                            android:textSize="@dimen/_15ssp"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="correct"
                            android:layout_marginLeft="@dimen/_5sdp"
                            android:textSize="@dimen/_15ssp"/>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="@dimen/_25sdp"
                            android:layout_height="@dimen/_25sdp"
                            android:src="@drawable/wrong_icon"/>

                        <TextView
                            android:id="@+id/total_wrong_question_id"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="0"
                            android:layout_marginLeft="@dimen/_2sdp"
                            android:textSize="@dimen/_15ssp"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Wrong"
                            android:layout_marginLeft="@dimen/_5sdp"
                            android:textSize="@dimen/_15ssp"/>

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="@dimen/_25sdp"
                            android:layout_height="@dimen/_25sdp"
                            android:src="@drawable/unanswered_icon"/>

                        <TextView
                            android:id="@+id/total_UnAnswered_question_id"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="0"
                            android:layout_marginLeft="@dimen/_2sdp"
                            android:textSize="@dimen/_15ssp"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Un-Answered"
                            android:layout_marginLeft="@dimen/_5sdp"
                            android:textSize="@dimen/_15ssp"/>

                    </LinearLayout>
<!--                    <LinearLayout-->
<!--                        android:layout_width="match_parent"-->
<!--                        android:layout_height="wrap_content"-->
<!--                        android:gravity="center_vertical"-->
<!--                        android:layout_marginTop="@dimen/_10sdp"-->
<!--                        android:orientation="horizontal">-->

<!--                        <ImageView-->
<!--                            android:layout_width="@dimen/_25sdp"-->
<!--                            android:layout_height="@dimen/_25sdp"-->
<!--                            android:src="@drawable/preview_icon"/>-->

<!--                        <TextView-->
<!--                            android:id="@+id/total_Preview_question_id"-->
<!--                            android:layout_width="wrap_content"-->
<!--                            android:layout_height="wrap_content"-->
<!--                            android:text="0"-->
<!--                            android:layout_marginLeft="@dimen/_2sdp"-->
<!--                            android:textSize="@dimen/_15ssp"/>-->
<!--                        <TextView-->
<!--                            android:layout_width="wrap_content"-->
<!--                            android:layout_height="wrap_content"-->
<!--                            android:text="Un-Answered"-->
<!--                            android:layout_marginLeft="@dimen/_5sdp"-->
<!--                            android:textSize="@dimen/_15ssp"/>-->

<!--                    </LinearLayout>-->

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"

                android:layout_marginTop="@dimen/_10sdp"
                android:weightSum="2"
                android:orientation="horizontal">


                <RelativeLayout
                    android:id="@+id/Ass_ReTestButton_id"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"

                    android:layout_marginLeft="@dimen/_10sdp"
                    android:layout_marginRight="@dimen/_5sdp"
                    android:background="@drawable/button_back_new" >


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Re-Test"
                        android:textSize="@dimen/_15ssp"
                        android:textColor="#454444"
                        android:layout_centerInParent="true"
                        android:paddingLeft="@dimen/_5sdp"
                        android:paddingRight="@dimen/_5sdp"
                        android:paddingTop="@dimen/_7sdp"
                        android:paddingBottom="@dimen/_7sdp"
                        android:textStyle="bold"/>
                </RelativeLayout>


                <RelativeLayout
                    android:id="@+id/Ass_solutionButton_id"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginLeft="@dimen/_5sdp"
                    android:layout_marginRight="@dimen/_10sdp"
                    android:backgroundTint="#2E8832"
                    android:background="@drawable/button_back_new" >



                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Solutions"
                        android:textSize="@dimen/_15ssp"
                        android:textColor="@color/white"
                        android:layout_centerInParent="true"
                        android:paddingLeft="@dimen/_5sdp"
                        android:paddingRight="@dimen/_5sdp"
                        android:paddingTop="@dimen/_7sdp"
                        android:paddingBottom="@dimen/_7sdp"
                        android:textStyle="bold"/>
                </RelativeLayout>


            </LinearLayout>






