    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginRight="@dimen/_5sdp"
        android:layout_marginLeft="@dimen/_5sdp"
        android:layout_marginTop="@dimen/_10sdp"
        android:layout_marginBottom="@dimen/_10sdp"
        app:cardCornerRadius="@dimen/_10sdp">


       <LinearLayout
           android:layout_width="match_parent"
           android:layout_height="wrap_content"
           android:orientation="vertical">

           <TextView
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:text="Analysis Report"
               android:textStyle="bold"
               android:layout_gravity="center_horizontal"
               android:textColor="#454444"
               android:textSize="@dimen/_20ssp"
               android:layout_marginLeft="@dimen/_15sdp"/>


           <LinearLayout
               android:layout_width="match_parent"
               android:layout_height="wrap_content"
               android:weightSum="5"
               android:orientation="horizontal">

               <TextView
                   android:layout_width="match_parent"
                   android:layout_height="wrap_content"
                   android:text="Accuracy"
                   android:layout_weight="4"
                   android:gravity="center"
                   android:layout_gravity="center"
                   android:textStyle="bold"
                   android:layout_marginLeft="@dimen/_10sdp"/>

               <com.daimajia.numberprogressbar.NumberProgressBar
                   android:id="@+id/numberbar1"
                   android:layout_width="match_parent"
                   android:layout_weight="1"
                   android:padding="20dp"
                   custom:progress_unreached_bar_height="2dp"
                   custom:progress_reached_bar_height="5dp"
                   custom:progress_current="50"
                   style="@style/NumberProgressBar_Default"
                   android:layout_height="wrap_content" />
           </LinearLayout>

           <LinearLayout
               android:layout_width="match_parent"
               android:layout_height="wrap_content"
               android:weightSum="5"
               android:orientation="horizontal">

               <TextView
                   android:layout_width="match_parent"
                   android:layout_height="wrap_content"
                   android:text="Time"
                   android:layout_weight="4"
                   android:gravity="center"
                   android:layout_gravity="center"
                   android:textStyle="bold"
                   android:layout_marginLeft="@dimen/_10sdp"/>

               <com.daimajia.numberprogressbar.NumberProgressBar
                   android:id="@+id/numberbar2"
                   android:layout_height="wrap_content"
                   android:padding="20dp"
                   android:layout_weight="1"
                   custom:progress_current="20"
                   custom:progress_unreached_bar_height="2dp"
                   custom:progress_reached_bar_height="5dp"
                   android:layout_width="match_parent"
                   style="@style/NumberProgressBar_Passing_Green"
                   />
           </LinearLayout>

           <LinearLayout
               android:layout_width="match_parent"
               android:layout_height="wrap_content"
               android:weightSum="5"
               android:orientation="horizontal">

               <TextView
                   android:layout_width="match_parent"
                   android:layout_height="wrap_content"
                   android:text="Correct"
                   android:layout_weight="4"
                   android:gravity="center"
                   android:layout_gravity="center"
                   android:textStyle="bold"
                   android:layout_marginLeft="@dimen/_10sdp"/>

               <com.daimajia.numberprogressbar.NumberProgressBar
                   android:id="@+id/numberbar3"
                   android:layout_margin="20dp"
                   android:layout_weight="1"
                   style="@style/NumberProgressBar_Relax_Blue"
                   custom:progress_current="30"
                   custom:progress_unreached_bar_height="2dp"
                   custom:progress_reached_bar_height="5dp"
                   android:layout_height="wrap_content" />
           </LinearLayout>

           <LinearLayout
               android:layout_width="match_parent"
               android:layout_height="wrap_content"
               android:weightSum="5"
               android:orientation="horizontal">

               <TextView
                   android:layout_width="match_parent"
                   android:layout_height="wrap_content"
                   android:text="Wrong"
                   android:layout_weight="4"
                   android:gravity="center"
                   android:layout_gravity="center"
                   android:textStyle="bold"
                   android:layout_marginLeft="@dimen/_10sdp"/>


               <com.daimajia.numberprogressbar.NumberProgressBar
                   android:id="@+id/numberbar4"
                   android:layout_width="wrap_content"
                   android:layout_margin="20dp"
                   android:layout_weight="1"
                   style="@style/NumberProgressBar_Grace_Yellow"
                   custom:progress_current="40"
                   custom:progress_unreached_bar_height="2dp"
                   custom:progress_reached_bar_height="7dp"
                   android:layout_height="wrap_content" />
           </LinearLayout>

           <LinearLayout
               android:layout_width="match_parent"
               android:layout_height="wrap_content"
               android:weightSum="5"
               android:orientation="horizontal">

               <TextView
                   android:layout_width="match_parent"
                   android:layout_height="wrap_content"
                   android:text="Preview"
                   android:layout_weight="4"
                   android:gravity="center"
                   android:layout_gravity="center"
                   android:textStyle="bold"
                   android:layout_marginLeft="@dimen/_10sdp"/>


               <com.daimajia.numberprogressbar.NumberProgressBar
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_margin="20dp"
                   android:layout_weight="1"
                   custom:progress_unreached_color="#CCCCCC"
                   custom:progress_reached_color="#3498DB"

                   custom:progress_unreached_bar_height="2dp"
                   custom:progress_reached_bar_height="7dp"

                   custom:progress_text_size="10sp"
                   custom:progress_text_color="#3498DB"
                   custom:progress_text_offset="1dp"
                   custom:progress_text_visibility="visible"

                   custom:progress_max="100"
                   custom:progress_current="80"
                   />
           </LinearLayout>


       </LinearLayout>

    </androidx.cardview.widget.CardView>


            <androidx.cardview.widget.CardView
                android:layout_marginTop="@dimen/_10sdp"
